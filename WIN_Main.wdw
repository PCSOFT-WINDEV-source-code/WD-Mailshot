  -
    name : BTN_Send
    identifier : 0x286fcad506005f93
    internal_properties : 
    properties :
     x : 9
     y : 529
     width : 75
     height : 24
    type : 4
    code_elements :
     type_code : 4
     p_codes :
      -
        internal_properties : CAAAAAgAAADh0+Qvs5QZBlHaN3ikr+7/ecsOPBZ357NhGLM++FVJEQteQKANqt1R0pnvJjQKA7xJ/LikJkoF2aGwG0xh/4vnTqbHKno3ZykLRTXjz9ntNXQud77TSll4+5vog+AIqFMSDtJYd7H0xIos3YY=
        code : |1+
         // Local variables
         bOK 				is boolean
         sMsg 				is string
         bAtLeastOneCustomer	is boolean = False
         
         // Check whether at least one customer is selected
         FOR ALL ROW of TABLE_Customers
         	// if the customer is part of the recipients
         	IF TABLE_Customers.COL_Checkmark THEN
         		bAtLeastOneCustomer = True
         	END
         END
         
         IF bAtLeastOneCustomer=False THEN
         	Info(<§$0017§>)
         	RETURN
         END
         
         HourGlass()
         
         // Stores the message-type
         INIWrite("EMAIL","Object_",EDT_Object,fCurrentDir()+"\Mailshot.ini")
         nFileID is int = fOpen("Message.txt", foCreate+foWrite)
         fWrite(nFileID, EDT_Message)
         fClose(nFileID)
         INIWrite("EMAIL","ATT",EDT_Attachments,fCurrentDir()+"\Mailshot.ini")
         
         
         // initialize the table
         FOR ALL ROW of TABLE_Customers
         	TABLE_Customers.COL_State = ""
         END
         
         // For each row of the table
         //note: <Control Name> returns the subscript of the current row. 
         
         FOR ALL ROW of TABLE_Customers
         	// if the customer is part of the recipients
         	IF TABLE_Customers.COL_Checkmark THEN
         		TableSelectPlus(TABLE_Customers,TABLE_Customers)
         		COL_State[TABLE_Customers]=<§$0005§>
         		// Customize the message
         		sMsg=sPrepareMessage(EDT_Message,TABLE_Customers)
         		// Send the message
         		bOK=bSendMessage(COL_Email[TABLE_Customers],EDT_Object,sMsg,EDT_Attachments)
         		IF NOT bOK THEN
         			IF NOT gbSessionOpen THEN
         				COL_State[TABLE_Customers]=<§$0007§>
         				BREAK
         			ELSE
         				COL_State[TABLE_Customers]=<§$0008§>
         			END
         		ELSE
         			COL_Checkmark[TABLE_Customers]=False
         			COL_State[TABLE_Customers]=<§$0009§>
         		END
         	END
         END
         
         HourGlass(False)
         
         
         IF bOK THEN 
         	Info(<§$000a§>)
         END
        type : 18
      -
        type : 161
        enabled : false
