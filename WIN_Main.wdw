   -
     name : bSendMessage
     procedure_id : 1760848995507658643
     type_code : 14
     code : |1+
      FUNCTION bSendMessage(sAddress,sObject,sContent,sATT="")
      
      // Send an email
      
      // Connection to the messaging if no current session
      IF NOT gbSessionOpen THEN
      	IF NOT Open(WIN_Connection) THEN RESULT False
      END
      
      // Reset the email structure to zero
      EmailReset()
      
      // Prepare the message
      Email.Sender = gsSender      		// The sender
      Email.SenderAddress = gsSender      
      Email.Recipient[1] = sAddress	        // The recipient
      Email.NbRecipient = 1                    // The number of recipients
      Email.Subject = sObject                        // The subject
      Email.Message = sContent                    // The body of the message
      
      // Attachments
      sAttList is string = sATT+";"
      sFile is string
      i is int
      Email.NbAttach = 0 
      LOOP
      	i++
      	sFile=NoSpace(ExtractString(sAttList,i,";"))
      	IF sFile = EOT THEN BREAK
      	IF fFileExist(sFile) = True THEN
      		Email.NbAttach++
      		Email.Attach[Email.NbAttach]=sFile
      	END
      END
      
      // Send the email
      RESULT EmailSendMessage(gsUser)
     type : 458752
   -
     name : sPrepareMessage
     procedure_id : 1760848995507724179
     type_code : 14
     code : |1+
      FUNCTION sPrepareMessage(sMessageType,nLine)
      
      // Replace the specific tags by their value in the file
      
      HReadSeek(CUSTOMER,CustomerNum,TABLE_Customers.COL_ID[nLine])
      sContent is string = sMessageType
      sContent = Replace(sContent, <§$0010§>,CUSTOMER.Title)
      sContent = Replace(sContent, <§$0011§>,CUSTOMER.Name)
      sContent = Replace(sContent, <§$0012§>,CUSTOMER.FirstName)
      sContent = Replace(sContent, <§$0013§>,CUSTOMER.Address)
      sContent = Replace(sContent, <§$0014§>,CUSTOMER.AddressNext)
      sContent = Replace(sContent, <§$0015§>,CUSTOMER.ZipCode)
      sContent = Replace(sContent, <§$0016§>,CUSTOMER.City)
      RESULT sContent
      
     type : 458752
